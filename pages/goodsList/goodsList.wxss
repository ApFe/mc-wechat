/* pages/goodsList/goodsList.wxss */
.goodList_box .goodlist_comm_box{
    background-color: #fff;
    width: 674rpx;
    height: 294rpx;
    border-radius: 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 30rpx;

}
.goodList_box .good_listImg{
    width: 201rpx;
    height: 201rpx;
    margin-left: 30rpx;
    margin-right: 30rpx;
}
.goodList_box .g_l_text_box{
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-right: 30rpx;
    height: 208rpx;
}
.goodList_box .gl_text_all{
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.goodList_box .g_l_text_one{
    width: 100%;
    font-size: 28rpx;
    line-height: 1.8;
    color: rgb(104, 104, 104);
}
.goodList_box .g_l_text_two{
    width: 100%;
    font-size: 24rpx;
    line-height: 2;
    color: rgb(177, 177, 177);
}
.goodList_box .g_l_text_btn_box{
    width: 100%;
    height: 50rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.g_l_text_btn_box .g_l_intger{
    flex: 1;
    font-size: 24rpx;
    color: rgb(82, 109, 226);
}
.g_l_text_btn_box .g_l_btn{
    width: 130rpx;
    height: 48rpx;
    line-height: 48rpx;
    text-align: center;
    font-size: 24rpx;
    color: #ffffff;
    border-radius: 24rpx;
    box-sizing: border-box;
    background: linear-gradient(to top right,rgb(250,159,0) ,rgb(249,202,0));
}
